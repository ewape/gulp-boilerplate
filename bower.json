{
    "name": "gulp-boilerplate",
    "repository": {
        "type": "git",
        "url": "git://github.com/ewape/gulp-boilerplate.git"
    },
    "authors": "Ewa Pettke",
    "keywords": "gulp",
    "description": "",
    "main": [
        "gulpfile.js",
        "package.json"
    ],
    "moduleType": [],
    "license": "MIT",
    "homepage": "",
    "private": true,
    "ignore": [
        "**/.*",
        "node_modules",
        "bower_components",
        "test",
        "tests",
        "dist",
        "build",
        "lib"
    ],
    "dependencies": {
        "jquery": "^3.1.0",
        "normalize-scss": "normalize.scss#^6.0.0"
    },
    "overrides": {
        "normalize-scss": {
            "main": [
                "sass/_normalize.scss",
                "sass/normalize/*.scss"
            ],
            "normalize": {
                "scss": ["*.scss"],
                "scss/normalize": ["**/normalize/*.scss"]
            }
        },
        "jquery": {
            "main": ["dist/jquery.min.js"]
        }
    }
}